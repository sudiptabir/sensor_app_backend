{
  "expo": {
    "name": "sensor_app",
    "slug": "sensor_app",
    "owner": "sudiptabir777",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sensorapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.gmail.sudiptabir.sensorapp",

      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },

    "plugins": [
      "expo-router",

      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],

      "expo-web-browser",

      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "968944435256-uu583289rveimgcvfg9m1k83ldrnbmmt.apps.googleusercontent.com",
      "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID": "968944435256-9k295e1jdfacv0tv3bs5luifsf8nmahl.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "61f78825-5a24-4965-9cac-2d11d2cf3391"
      }
    }
  }
}
